{
  "name": "conpack",
  "version": "0.0.1",
  "description": "Simple webpack config manager.",
  "repository": {
    "url": "egoist/conpack",
    "type": "git"
  },
  "main": "dist/conpack.cjs.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest && npm run lint && npm run build",
    "lint": "xo src/*.js",
    "build": "bili --js babel"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "external-helpers"
    ],
    "env": {
      "test": {
        "presets": [
          "latest",
          "stage-2"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-jest": "^18.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "bili": "^0.12.0",
    "jest-cli": "^18.1.0",
    "rollup-plugin-babel": "^2.7.1",
    "xo": "^0.17.1"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "esnext": true,
    "globals": [
      "test",
      "expect"
    ]
  },
  "dependencies": {
    "dot-prop": "^4.1.0",
    "is-equal-regex": "^0.1.0",
    "map-to-obj": "^0.2.0",
    "serialize-javascript": "^1.3.0"
  },
  "engine": {
    "node": ">=4"
  }
}
